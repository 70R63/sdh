					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="agentList"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="networkList"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="rolList"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="exteriorPublicityTaxList"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="gasTaxList"/>
