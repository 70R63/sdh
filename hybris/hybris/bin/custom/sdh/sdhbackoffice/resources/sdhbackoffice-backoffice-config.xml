	<context merge-by="type"  type="Customer" component="editor-area" module="sdhbackoffice">
        <editorArea:editorArea xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea" xmlns:na="http://www.hybris.com/cockpitng/config/notifications">
            <editorArea:tab name="hmc.tab.sdh.properties">
                <editorArea:section name="hmc.tab.sdh.administration">
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="numBP"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="documentType"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="documentNumber"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="documentExpeditionDate"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="firstName"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="middleName"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="lastName"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="secondLastName"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="sex"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="nationality"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="birthDate"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="birthPlace"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="originCountry"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="useEmailForNotifications"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="useInformationForInstitutionalPurposes"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="sdhTitle"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="nameOrg1"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="nameOrg2"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="nameOrg3"/>
					<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="nameOrg4"/>
