	<!-- Wizards -->
	<context component="create-wizard" type="SdhFAQsCategory">
		<wz:flow id="sdhFAQsCategoryTypeWizard" title="create.title(ctx.TYPE_CODE)">
			<wz:prepare id="sdhFAQsCategoryTypeWizard">
				<wz:initialize property="newSdhFAQsCategory" type="ctx.TYPE_CODE"/>
			</wz:prepare>
			<wz:step id="step1" label="create.essential.label">
				<wz:content id="step1.content">
					<wz:property-list root="newSdhFAQsCategory">
						<wz:property qualifier="code"/>
						<wz:property qualifier="description"/>
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel/>
					<wz:done visible="newSdhFAQsCategory.code != null and newSdhFAQsCategory.description != null">
						<wz:save property="newSdhFAQsCategory"/>
					</wz:done>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>
	<context component="create-wizard" type="SdhFAQs">
		<wz:flow id="SdhFAQsTypeWizard" title="create.title(ctx.TYPE_CODE)">
			<wz:prepare id="SdhFAQsTypeWizard">
				<wz:initialize property="newSdhFAQs" type="ctx.TYPE_CODE"/>
			</wz:prepare>
			<wz:step id="step1" label="create.essential.label">
				<wz:content id="step1.content">
					<wz:property-list root="newSdhFAQs">
						<wz:property qualifier="code"/>
						<wz:property qualifier="category"/>
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel/>
					<wz:next visible="newSdhFAQs.code != null and newSdhFAQs.category != null"/>
				</wz:navigation>
			</wz:step>
			<wz:step id="step2" label="create.SdhFAQs.label">
				<wz:content id="step2.content">
					<wz:property-list root="newSdhFAQs">
						<wz:property qualifier="question"/>
						<wz:property qualifier="answer" editor="com.hybris.cockpitng.editor.wysiwyg"/>
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step2.navigation">
					<wz:cancel/>
					<wz:back/>
					<wz:done>
						<wz:save property="newSdhFAQs"/>
					</wz:done>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>

