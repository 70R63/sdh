	 <context merge-by="type" parent="GenericItem" type="SDHAddress" component="base" module="sdhbackoffice">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris" xmlns:na="http://www.hybris.com/cockpitng/config/notifications">
            <y:labels>
                <y:label>addKind+' '+street</y:label>
			</y:labels>
		</y:base>
    </context>
    <context merge-by="type" parent="GenericItem" type="SDHPhone" component="base" module="sdhbackoffice">
