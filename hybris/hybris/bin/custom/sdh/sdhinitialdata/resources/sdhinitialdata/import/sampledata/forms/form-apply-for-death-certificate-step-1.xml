                            <label>Ciudad</label>
                            <hint/>
                            <alert>Introduzca un pueblo o una ciudad</alert>
                        </city-1>
                        <postcode-1>
                            <label>Código postal</label>
                            <hint/>
                            <alert>Introduzca un código postal</alert>
                        </postcode-1>
